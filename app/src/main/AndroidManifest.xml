<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:node="merge">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.SistemPakarParuParu"
        tools:ignore="HardcodedDebugMode,UnusedAttribute"
        tools:replace="android:appComponentFactory">
        <activity
            android:name=".sertifikat"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".kelompok14"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PenyakitDetail"
            android:parentActivityName=".PenyakitActivity" />
        <activity
            android:name=".DiagnosaCfHasilActivity"
            android:parentActivityName=".DiagnosaCfActivity"
            android:theme="@style/DiagnosaCfHasilTheme" />
        <activity
            android:name=".PenyakitDetailActivity"
            android:parentActivityName=".PenyakitActivity" />
        <activity
            android:name=".PenyakitActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RiwayatActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TentangActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DiagnosaCfActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DaftarActivity"
            android:theme="@style/DaftarTheme" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/MainTheme" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>